{
  "name": "@tippingchain/ui-react",
  "version": "2.0.0",
  "type": "commonjs",
  "description": "TippingChain UI v2.0 - React components for multi-chain tipping with USDC payouts",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "author": "TippingChain Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tippingchain/tippingchain.git",
    "directory": "packages/ui"
  },
  "homepage": "https://github.com/tippingchain/tippingchain#readme",
  "bugs": {
    "url": "https://github.com/tippingchain/tippingchain/issues"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "./node_modules/.bin/tsc",
    "dev": "rollup -c -w",
    "test": "jest",
    "lint": "./node_modules/.bin/eslint src/**/*.{ts,tsx}",
    "lint:fix": "./node_modules/.bin/eslint src/**/*.{ts,tsx} --fix",
    "typecheck": "./node_modules/.bin/tsc --noEmit",
    "prepublishOnly": "rm -rf dist && npm run typecheck && npm run build",
    "clean": "rm -rf dist"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "react",
    "tipping",
    "apechain",
    "web3",
    "ui",
    "components",
    "multi-chain",
    "thirdweb",
    "crypto",
    "defi"
  ],
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "thirdweb": "^5.0.0"
  },
  "dependencies": {
    "@tippingchain/contracts-interface": "^1.2.0",
    "@tippingchain/sdk": "^2.0.1",
    "clsx": "^2.0.0",
    "lucide-react": "^0.263.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.0.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "rimraf": "^5.0.0",
    "rollup": "^4.0.0",
    "rollup-plugin-dts": "^6.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  }
